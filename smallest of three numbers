 #include<stdio.h>
    int main(){
                  int n1, n2, n3;
                  printf(" Enter three Integer numbers  : ");
                  scanf("%d %d %d", &n1, &n2, &n3);
                  if((n1 < n2) &&(n1 < n3))
                  printf("\n n1 is smallest : %d",n1);
                  else
                 {
                 if(n2< n3)
                 printf("\n n2 is smallest : %d",n2);
                 else
                 printf("\n n3 is smallest : %d",n3);
                 }
    return(0);
    }
