# include<stdio.h>
# include<conio.h>
int main()
{
printf("enter any three numbers");
scanf("%d %d %d ", &n1 &n2 &n3);
if ( n1<n2 && n1<n3)
     printf("%d is smallest number",n1);
else if (n2<n1 && n2<n3)
     printf("%d is smallest number",n2);
else
     printf(" %d is smallest number",n3);
