#include<stdio.h>
#include<conio.h>
#include<math.h>

void sumofsquares(int);

int main ()
{
    int n;
    printf("enter value of n\n");
    scanf("%d",&n);
    sumofsquares(n);
    getch();
    return 0;
}
void sumofsquares(int a)
{
    float sum = 0; int i=1;
    while(i<=a)
    {
        if (i%2!=0)
        {
            sum=sum+pow(i,2);
        }
         i++;
    }
  printf("sum of squares of all odd numbers %d is = %f\n",a,sum);
  getch();
  return 0;
}




